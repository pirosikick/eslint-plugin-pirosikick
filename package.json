{
  "name": "eslint-plugin-pirosikick",
  "version": "0.0.1",
  "description": "The custom ESLint rules created by pirosikick",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Hiroyuki ANAI<pirosikick@gmail.com>",
  "main": "lib/index.js",
  "files": ["lib", "README.md"],
  "scripts": {
    "test": "mocha --compilers js:babel-core/register tests/**/*.js",
    "watch-test": "mocha -w --compilers js:babel-core/register tests/**/*.js",
    "build": "babel src -d lib",
    "watch-build": "babel -w src -d lib",
    "prepublish": "npm test && npm run build"
  },
  "dependencies": {
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.0.12",
    "babel-eslint": "^4.1.3",
    "babel-preset-es2015": "~6.0.12",
    "eslint": "^1.2.1",
    "espower-babel": "^3.3.0",
    "mocha": "^2.3.3"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT"
}
